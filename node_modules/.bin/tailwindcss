#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/private/tmp/honghu_container_rental_poc/.pnpm/tailwindcss@3.4.16/node_modules/tailwindcss/lib/node_modules:/private/tmp/honghu_container_rental_poc/.pnpm/tailwindcss@3.4.16/node_modules/tailwindcss/node_modules:/private/tmp/honghu_container_rental_poc/.pnpm/tailwindcss@3.4.16/node_modules:/private/tmp/honghu_container_rental_poc/.pnpm/node_modules:/private/tmp/honghu_container_rental_poc/node_modules:/private/tmp/node_modules:/private/node_modules:/node_modules:/tmp/honghu_container_rental_poc/.pnpm/node_modules"
else
  export NODE_PATH="/private/tmp/honghu_container_rental_poc/.pnpm/tailwindcss@3.4.16/node_modules/tailwindcss/lib/node_modules:/private/tmp/honghu_container_rental_poc/.pnpm/tailwindcss@3.4.16/node_modules/tailwindcss/node_modules:/private/tmp/honghu_container_rental_poc/.pnpm/tailwindcss@3.4.16/node_modules:/private/tmp/honghu_container_rental_poc/.pnpm/node_modules:/private/tmp/honghu_container_rental_poc/node_modules:/private/tmp/node_modules:/private/node_modules:/node_modules:/tmp/honghu_container_rental_poc/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tailwindcss/lib/cli.js" "$@"
else
  exec node  "$basedir/../tailwindcss/lib/cli.js" "$@"
fi
